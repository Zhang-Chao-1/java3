package shiyan4;

public class Open extends basic {
	 public String courses = "";
	 Open(String name,String sex,int number,String cours) {
	  super();
	     this.name=name;
	     this.sex=sex;
	     this.number=number;
	     this.cours=cours;
	 }
	 void setCourse(String keming) {
	  this.courses=keming;
	 }
	 
}
